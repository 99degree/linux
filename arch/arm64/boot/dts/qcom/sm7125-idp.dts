	status = "okay";

	vdda-supply = <&vreg_l3c_1p2>;
	vddio-supply = <&vreg_l18a_2p8>;
	vdds-supply = <&vreg_l18a_2p8>;

	panel0: panel@0 {
		compatible = "mdss,nt36675-tianma", "xiaomi,nt36675-tianma-dummy", "mdss,nt36675-tianma-fhd";

		reg = <0>;

		vdda-supply = <&vreg_l8c_1p8>;
