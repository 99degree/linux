&soc {
	dummy_clk: dummy_clk {
		compatible = "fixed-factor-clock";
	        clocks = <&dispcc DISP_CC_MDSS_AHB_CLK>, <&dispcc DISP_CC_MDSS_NON_GDSC_AHB_CLK>, <&dispcc DISP_CC_MDSS_MDP_LUT_CLK>;
	        #clock-cells = <2>;
	        clock-div = <1>;
	        clock-mult = <1>;
	};
};

