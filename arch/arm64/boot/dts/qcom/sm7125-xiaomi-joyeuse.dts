#ifndef _SM7125_XIAOMI_JOYEUSE_DTS_
#define _SM7125_XIAOMI_JOYEUSE_DTS_

/dts-v1/;

#include "sm7125-xiaomi-common.dtsi"

/ {
	model = "Xiaomi Redmi Note 9 Pro (Global)";
	compatible = "xiaomi,joyeuse", "qcom,sm7125", "qcom,sc7180";

	/*
	 * this is required for bootloader to select correct board, 
	 * but if setting to 0 bootloader also can work with it like 
	 * vendor kernel. So there is a chance to get going dt overlay.
	 */
	qcom,board-id = <0x50022 1>;
	//qcom,board-id = <0 0>;
};

#endif
